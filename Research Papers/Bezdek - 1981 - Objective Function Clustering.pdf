          if (isReferenceWorkEntry && !isEncOrDict && hasBody) {
            reportParticipation('https://ab-reporting.live.cf.public.springer.com', 'bunsen-'+liveOrStatic+'-rwepage-nonenc-html-download', 'baseline');
